@model IEnumerable<dynamic>

<h2>Результаты левого внешнего соединения (Albums, Groups и Companies)</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID Альбома</th>
            <th>Название Альбома</th>
            <th>Дата выпуска</th>
            <th>ID Группы</th>
            <th>Название Группы</th>
            <th>ID Компании</th>
            <th>Название Компании</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.AlbumId</td>
                <td>@item.AlbumName</td>
                <td>@item.ReleaseDate</td>
                <td>@item.GroupId</td>
                <td>@item.GroupName</td>
                <td>@item.CompanyId</td>
                <td>@item.CompanyName</td>
            </tr>
        }
    </tbody>
</table>